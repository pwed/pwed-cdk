{
  "name": "pwed-cdk",
  "version": "0.1.27",
  "description": "A library of AWS CDK constructs that I have created",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "prepare": "tsc",
    "build": "tsc",
    "test": "jest",
    "clean": "prettier --config .prettierrc 'lib/**/*.ts' --ignore-path '/lib/**/*.d.ts' --write "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com:pwed/pwed-cdk.git"
  },
  "keywords": [
    "cdk",
    "constructs",
    "bastion",
    "AWS SSO",
    "static site",
    "AWS",
    "Jump Host"
  ],
  "author": "pwed",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pwed/pwed-cdk/issues"
  },
  "homepage": "https://github.com/pwed/pwed-cdk#readme",
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "aws-cdk-lib": "2.10.0",
    "cdk-constants": "3.0.3",
    "cdk-ec2-key-pair": "3.2.0",
    "constructs": "^10.0.72",
    "glob": "7.2.0",
    "jest": "^26.4.2",
    "jsii": "^1.54.0",
    "jsii-pacmak": "^1.54.0",
    "jsii-release": "^0.2.271",
    "prettier": "^2.5.1",
    "ts-jest": "^26.2.0",
    "typescript": "~3.9.7"
  },
  "peerDependencies": {
    "aws-cdk-lib": "^2.10.0",
    "constructs": "^10.0.5"
  },
  "dependencies": {
    "cdk-ec2-key-pair": "^3.2.0",
    "cdk-constants": "^3.0.3"
  },
  "bundleDependencies": [
    "glob"
  ],
  "stability": "experimental",
  "jsii": {
    "outdir": "jsii",
    "versionFormat": "short",
    "targets": {
      "python": {
        "distName": "pwed-cdk",
        "module": "pwed-cdk"
      }
    }
  }
}
